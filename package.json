{
    "name": "Flashorama",
    "productName": "Flashorama",
    "version": "1.1.4",
    "description": "Flashorama",
    "main": "src/main.js",
    "scripts": {
        "start": "electron-forge start",
        "package": "electron-forge package",
        "make": "electron-forge make",
        "publish": "electron-forge publish",
        "publish-windows": "electron-forge publish",
        "publish-mac": "electron-forge publish --arch=x64",
        "publish-git": "electron-forge publish",
        "lint": "echo \"No linting configured\"",
        "release": "release-it"
    },
    "keywords": [
        "Flashorama",
        "Flash",
        "Game"
    ],
    "author": "LightShoro & Heaventy",
    "license": "MIT",
    "config": {
        "forge": {
            "packagerConfig": {
                "icon": "lib/icons/icon"
            },
            "makers": [{
                "name": "@electron-forge/maker-squirrel",
                "config": {
                    "name": "heaventyflashorama",
                    "authors": "LightShoro",
                    "loadingGif": "./src/assets/default-splash.gif",
                    "iconUrl": "https://flashorama.heaventy-projects.fr/assets/app.ico",
                    "setupIcon": "./lib/icons/icon.ico"
                }
            }, {
                "name": "@electron-forge/maker-zip",
                "platforms": [
                    "darwin"
                ]
            }, {
                "name": "@electron-forge/maker-dmg",
                "config": {
                    "background": "./lib/icons/macos_bg.png",
                    "format": "ULFO"
                }
            }, {
                "name": "@electron-forge/maker-deb",
                "config": {}
            }, {
                "name": "@electron-forge/maker-rpm",
                "config": {}
            }, {
                "name": "@electron-forge/maker-flatpak",
                "config": {
                    "genericName": "Flashorama",
                    "productName": "Flashorama",
                    "id": "fr.lightshoro.heaventy.Flashorama",
                    "options": [{
                        "id": "fr.lightshoro.heaventy.Flashorama"
                    }],
                    "categories": [
                        "Game"
                    ],
                    "modules": [{
                        "name": "zypak",
                        "sources": [{
                            "type": "git",
                            "url": "https://github.com/refi64/zypak",
                            "tag": "v2022.04"
                        }]
                    }],
                    "runtimeVersion": "22.08",
                    "baseVersion": "22.08",
                    "icon": "./lib/icons/icon.ico"
                }
            }],
            "publishers": [{
                "name": "@electron-forge/publisher-github",
                "config": {
                    "repository": {
                        "owner": "DarkShoro",
                        "name": "HeaventyFlashorama"
                    },
                    "prerelease": true
                }
            }]
        }
    },
    "build": {
        "extraResources": [{
            "from": "./lib/",
            "to": "lib",
            "filter": [
                "**/*"
            ]
        }],
        "appId": "fr.lightshoro.heaventy.flashorama",
        "productName": "Heaventy's Projects : Flashorama",
        "copyright": "Copyright Â© 2024 ${author}",
        "mac": {
            "icon": "./lib/icons/icon.icns",
            "category": "public.app-category.games",
            "target": [
                "dmg"
            ]
        },
        "dmg": {
            "background": "./lib/icons/macos_bg.png"
        },
        "win": {
            "icon": "./lib/icons/icon.png"
        },
        "directories": {
            "buildResources": "lib"
        },
        "protocols": [{
            "name": "flashorama",
            "schemes": [
                "flashorama"
            ]
        }]
    },
    "dependencies": {
        "abort-controller": "^3.0.0",
        "async-dialog": "0.0.1",
        "dialogs": "^2.0.1",
        "discord-rpc": "^4.0.1",
        "electron-prompt": "^1.6.0",
        "electron-squirrel-startup": "^1.0.0",
        "es5-ext": "<=0.10.53",
        "node-gyp": "8.4.1",
        "update-electron-app": "^2.0.1",
        "yargs": "^17.7.2"
    },
    "devDependencies": {
        "@electron-forge/cli": "^6.0.0-beta.54",
        "@electron-forge/maker-appx": "^6.0.0-beta.54",
        "@electron-forge/maker-deb": "^6.0.0-beta.54",
        "@electron-forge/maker-dmg": "^6.0.0-beta.54",
        "@electron-forge/maker-flatpak": "^6.0.0-beta.64",
        "@electron-forge/maker-rpm": "^6.0.0-beta.54",
        "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
        "@electron-forge/maker-zip": "^6.0.0-beta.54",
        "@electron-forge/publisher-github": "^6.0.0-beta.54",
        "electron": "11.1.1"
    }
}
